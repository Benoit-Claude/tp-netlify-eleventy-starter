{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "description": "A starter project built with Eleventy, Webpack and PostCSS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/eastslopestudio/eleventy-starter"
  },
  "scripts": {
    "clean": "del dist",
    "dev:postcss": "postcss src/_assets/css/app.css -o src/static/app.compiled.css --watch --verbose",
    "dev:scripts": "webpack --watch --config webpack.config.js",
    "dev:11ty": "eleventy --serve --watch",
    "build:postcss": "NODE_ENV=production postcss src/_assets/css/app.css -o src/_includes/app.compiled.css",
    "build:11ty": "ELEVENTY_ENV=production eleventy",
    "build:scripts": "NODE_ENV=production webpack --config webpack.config.js",
    "start": "run-p dev:* --print-label",
    "build": "run-s clean build:* --print-label",
    "tw-config": "tailwind-config-viewer -o"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "alpinejs": "^2.8.2",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "del-cli": "^3.0.1",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "focus-visible": "^5.2.0",
    "html-minifier": "^4.0.0",
    "js-cookie": "^2.2.1",
    "luxon": "^1.27.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "tailwind-config-viewer": "^1.5.1",
    "tailwindcss": "^2.1.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  }
}
